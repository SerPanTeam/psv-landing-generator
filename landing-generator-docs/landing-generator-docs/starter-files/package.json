{
  "name": "landing-generator",
  "version": "1.0.0",
  "description": "Landing page generator for photographers with quiz functionality",
  "main": "generator.js",
  "scripts": {
    "build": "node generator.js",
    "build:all": "node generator.js --all",
    "serve": "npx serve dist -p 3000",
    "dev": "npm run build && npm run serve",
    "test": "npx playwright test",
    "test:update": "npx playwright test --update-snapshots",
    "lint": "npx eslint js/*.js",
    "deploy": "npm run build:all && gh-pages -d dist"
  },
  "keywords": [
    "landing-page",
    "generator",
    "bootstrap",
    "quiz",
    "photographer"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "eslint": "^8.55.0",
    "gh-pages": "^6.1.0",
    "pixelmatch": "^5.3.0",
    "pngjs": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
