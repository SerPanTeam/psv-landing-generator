{
  "name": "landing-generator",
  "version": "1.0.0",
  "description": "Landing page generator for photographers based on Bootstrap 5",
  "main": "generator.js",
  "scripts": {
    "build": "node generator.js",
    "build:landing1": "node generator.js --landing=1",
    "build:landing2": "node generator.js --landing=2",
    "build:landing3": "node generator.js --landing=3",
    "build:landing4": "node generator.js --landing=4",
    "serve": "npx serve dist -p 8080",
    "watch": "node generator.js --watch",
    "test": "node test-comprehensive.js",
    "test:content": "node test-figma-content.js",
    "test:backgrounds": "node test-backgrounds.js",
    "test:mobile": "node take-screenshots.js",
    "test:desktop": "node take-desktop-screenshot.js",
    "test:figma": "node test-figma-comparison.js",
    "test:all": "npm run build && npm run test",
    "audit": "npm run build && npm run test"
  },
  "keywords": [
    "landing",
    "generator",
    "bootstrap",
    "photography"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "^1.40.0"
  }
}
